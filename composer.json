{
	"name":"blend/ezquerybuilderbundle",
    "type": "symfony-bundle",
    "description": "Syntactic Sugar Fluent query interface for the eZ 5 Public API",
    "keywords": ["ezpublish", "bundle", "query"],
    "homepage": "http://github.com/blendinteractive/blendezquerybuilderbundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Kepley",
            "email": "joe@blendinteractive.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/joekepley/ezpublish-kernel.git"
        }
    ],
	"require": {
        "php": ">=5.3.3",
		"blend/ezquerybuilderbundle": "dev-master"
	},
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Blend\\Bundle\\EzQueryBuilderBundle": "" }
    },
    "target-dir": "Blend/Bundle/EzQueryBuilderBundle"

}
