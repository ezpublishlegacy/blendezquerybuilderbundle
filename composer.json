{
	"name":"blend/ezquerybuilderbundle",
    "type": "symfony-bundle",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/joekepley/ezpublish-kernel.git"
        }
    ],
	"require": {
        "php": ">=5.3.3",
        "ezsystems/ezpublish-kernel": "dev-master"
	},
    "minimum-stability": "dev",
    "config": {
        "process-timeout": 3000
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Blend\\Bundle\\EzQueryBuilderBundle": "" }
    },
    "target-dir": "Blend/Bundle/EzQueryBuilderBundle"

}
